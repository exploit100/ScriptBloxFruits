local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("AutoFarm Made By HeartI#5957", "BloodTheme")

--MAIN
local Tab = Window:NewTab("Main")


--BLOX FRUIT
local Tab = Window:NewTab("BloxFruits")
local Tab = Tab:NewSection("Exploits")

Tab:NewButton("Redz Hub", "Redz Hub", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/BloxFruits/main/redz9999"))()
end)

Tab:NewButton("Hoho Hub", "Hoho Hub", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
end)

Tab:NewButton("Alchemy Hub", "Alchemy Hub", function()
    loadstring(game:HttpGet("https://luable.top/acm"))()
end)
Tab:NewButton("Unknown V4 Hub", "Unknown V4 Hub", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/BestScriptEverr/Main-/main/UnknownHubV3"))()
end)

Tab:NewButton("Z Azure Hub", "Z Azure Hub", function()
    loadstring(game:HttpGet("https://shz.al/dwdT/MainLoader.lua"))()
end)
