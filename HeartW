local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("AutoFarm", "BloodTheme")

--MAIN
local Tab = Window:NewTab("Main")


--BLOX FRUIT
local Tab = Window:NewTab("BloxFruits")
local Tab = Tab:NewSection("Exploits")

Tab:NewButton("New Mukuro", "Mukuro Hub", function()
    loadstring(game:HttpGet"https://raw.githubusercontent.com/xQuartyx/DonateMe/main/ScriptLoader")()
end)

Tab:NewButton("Old Mukuro", "Mukuro Hub", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/xQuartyx/DonateMe/main/OldBf"))()
end)

Tab:NewButton("Ripper V2", "Ripper Hub", function()
    _G.Version = "V2" -- V2 or V3
loadstring(game:HttpGet('https://raw.githubusercontent.com/xDestinyx/RipperHub/main/Loader.lua'))();
end)

Tab:NewButton("Ripper V3", "Ripper Hub", function()
    _G.Version = "V3" -- V2 or V3
loadstring(game:HttpGet('https://raw.githubusercontent.com/xDestinyx/RipperHub/main/Loader.lua'))();
end)

Tab:NewButton("ThunderZ", "ThunderZ", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ThunderZ-05/HUB/main/Script"))()
end)

